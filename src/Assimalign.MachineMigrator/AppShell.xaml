<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:AppShell="http://schemas.microsoft.com/winfx/2009/xaml"
    AppShell:Class="Assimalign.MachineMigrator.AppShell"
    xmlns:pages="clr-namespace:Assimalign.MachineMigrator.Pages"
    Shell.FlyoutBehavior="Locked"
    Shell.FlyoutWidth="200">

    <FlyoutItem Title="Applications" Route="applications">
        <ShellContent Title="Applications" 
                      ContentTemplate="{DataTemplate pages:ApplicationsPage}" 
                      Route="applications" />
    </FlyoutItem>
    <FlyoutItem Title="Github" Route="gtihub">
        <ShellContent Title="Github" 
                      ContentTemplate="{DataTemplate pages:GithubPage}" 
                      Route="github"/>
    </FlyoutItem>

</Shell>
